<UserControl x:Class="rtwin.View.master.shift.FormShiftHarian"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:rtwin.View.master.shift"
             mc:Ignorable="d" 
              xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             d:DesignHeight="970" d:DesignWidth="700">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"></ColumnDefinition>
            <ColumnDefinition Width="auto"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Row="0" Grid.Column="0" Margin="5,10,0,0" Orientation="Vertical">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"></RowDefinition>
                    <RowDefinition Height="auto"></RowDefinition>
                    <RowDefinition Height="auto"></RowDefinition>
                    <RowDefinition Height="auto"></RowDefinition>
                    <RowDefinition Height="auto"></RowDefinition>
                    <RowDefinition Height="auto"></RowDefinition>
                    <RowDefinition Height="auto"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="125"></ColumnDefinition>
                    <ColumnDefinition Width="160"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <StackPanel Grid.Row="0" Grid.Column="0" Margin="10,10,10,0">
                    <TextBlock Style="{StaticResource MaterialDesignSubheadingTextBlock}"> Grup (Shift)
                    </TextBlock>
                </StackPanel>
                <StackPanel Grid.Row="0" Grid.Column="1" Margin="10,0,10,10">
                    <TextBox x:Name="txtKodeShift" Style="{StaticResource MaterialDesignFloatingHintTextBox}" />
                </StackPanel>
                <StackPanel Grid.Row="1" Grid.Column="0" Margin="10,10,10,0">
                    <TextBlock Style="{StaticResource MaterialDesignSubheadingTextBlock}"> Pola
                    </TextBlock>
                </StackPanel>
                <StackPanel Grid.Row="1" Grid.Column="1" Margin="10,10,10,0">
                    <TextBlock Style="{StaticResource MaterialDesignSubheadingTextBlock}">N(harian)
                        </TextBlock>
                </StackPanel>
                <StackPanel Grid.Row="2" Grid.Column="0" Margin="10,10,10,0">
                    <TextBlock Style="{StaticResource MaterialDesignSubheadingTextBlock}"> Tanggal Berlaku
                    </TextBlock>
                </StackPanel>
                <StackPanel Grid.Row="2" Grid.Column="1" Margin="0,10,10,10">
                    <DatePicker x:Name="tglBerlaku" />
                </StackPanel>
                <StackPanel Grid.Row="3" Grid.Column="0" Margin="10,10,10,0">
                    <TextBlock Style="{StaticResource MaterialDesignSubheadingTextBlock}"> Nama Grup
                    </TextBlock>
                </StackPanel>
                <StackPanel Grid.Row="3" Grid.Column="1" Margin="10,0,10,10">
                    <TextBox x:Name="txtNamaGrup" Style="{StaticResource MaterialDesignFloatingHintTextBox}" />
                </StackPanel>
                <StackPanel Grid.Row="4" Grid.Column="0" Margin="10,10,10,0">
                    <TextBlock Style="{StaticResource MaterialDesignSubheadingTextBlock}"> Nama Satker
                    </TextBlock>
                </StackPanel>
                <StackPanel Grid.Row="4" Grid.Column="1" Margin="10,0,10,10">
                    <ComboBox x:Name="cmbNamaSatker" SelectedValuePath="Content"></ComboBox>
                </StackPanel>
                <StackPanel Grid.Row="5" Grid.Column="0" Margin="10,10,10,0">
                    <TextBlock Style="{StaticResource MaterialDesignSubheadingTextBlock}"> Libur Nasional</TextBlock>
                </StackPanel>
                <StackPanel Grid.Row="5" Grid.Column="1" Margin="10,10,10,0" HorizontalAlignment="Left">
                    <ToggleButton x:Name="chkLiburNasional" Style="{StaticResource MaterialDesignSwitchAccentToggleButton}" ToolTip="MaterialDesignSwitchAccentToggleButton" IsChecked="True" />
                </StackPanel>
                <StackPanel Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="2" Margin="10,10,10,0" Orientation="Horizontal">
                    <Button x:Name="btnsave" Click="Btnsave_Click" Style="{StaticResource GreenButton}" Width="70" ToolTip="Resource name: MaterialDesignRaisedButton">
                        Save
                    </Button>
                    <Button x:Name="btnClose" Margin="10" Click="BtnClose_Click" Style="{StaticResource  MaterialDesignRaisedButton}" Background="Red" BorderBrush="Red" Width="70" ToolTip="Resource name: MaterialDesignRaisedButton">
                        Close
                    </Button>
                    <StackPanel Margin="0,10,0,0" Grid.Column="1" Orientation="Horizontal" x:Name="lodongSave"  Visibility="Hidden" >
                        <materialDesign:Card UniformCornerRadius="14" Padding="4"  >
                            <ProgressBar Style="{StaticResource MaterialDesignCircularProgressBar}" Value="50" IsIndeterminate="True" />
                        </materialDesign:Card>

                    </StackPanel>
                </StackPanel>
            </Grid>
        </StackPanel>

